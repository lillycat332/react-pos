{
  "name": "triangle-pos",
  "version": "1.0.0",
  "main": "./dist/main.js",
  "license": "BSD-3-Clause",
  "private": true,
  "build": {
    "extraResources": [
      "./bin/**"
    ],
    "appId": "com.triangle.pos"
  },
  "devDependencies": {
    "electron": "^21.1.1",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "smalltalk": "^4.1.1"
  },
  "electronPackagerConfig": {
    "asar": true,
    "asarUnpack": [
      "bin/**"
    ],
    "icon": "./assets/icon",
    "ignore": [
      "node_modules",
      "src",
      "dist",
      "assets",
      "bin",
      "package-lock.json",
      "package.json",
      "webpack.config.js"
    ],
    "extraResource": [
      "./bin/**"
    ]
  }
}
